<% /* file: views/partials/user-list.ejs              */ %>
<!-- begin: user-list -->
   		  <ul class="nav navbar-nav">
          <li class="menu-item dropdown">
            <a class="dropdown-toggle" data-toggle="dropdown" href="">Site Members <span class="caret"></span></a>
            <ul class="dropdown-menu">
              <li class="menu-item dropdown dropdown-submenu">
                <a class="dropdown-toggle" data-toggle="dropdown" href="">Users</a>
                <ul class="dropdown-menu"><%
users.forEach(function(user) {
  if (!user.is_admin && !user.is_suspended) { %>
                  <li class="menu-item"><a href="/user/<%= user._id %>"><%= user.username %></a></li><%
  }
}) %>
                </ul>
              </li>
              <li class="menu-item dropdown dropdown-submenu">
                <a class="dropdown-toggle" data-toggle="dropdown" href="">Admins</a>
                <ul class="dropdown-menu"><%
users.forEach(function(user) {
  if (user.is_admin && !user.is_suspended) { %>
                  <li class="menu-item item-green"><a href="/user/<%= user._id %>"><%= user.username %></a></li><%
  }
}) %>
                </ul>
              </li>
              <li class="menu-item dropdown dropdown-submenu">
                <a class="dropdown-toggle" data-toggle="dropdown" href="">Suspended</a>
                <ul class="dropdown-menu"><%
users.forEach(function(user) {
if (user.is_suspended) { %>
                  <li class="menu-item item-red"><a href="/user/<%= user._id %>"><%= user.username %></a></li><%
  }
}) %>
                </ul><!-- /.dropdown-menu -->
              </li><!-- /.menu-item /.dropdown /.dropdown-submenu -->
            </ul><!-- /.dropdown-menu -->
          </li><!-- /.menu-item /.dropdown -->
        </ul><!-- /.nav /.navbar-nav -->
<!-- end: user-list -->